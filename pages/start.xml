<fable>
  <config font-family="Creepster" />
  <page id="start" bg_music="osts-fear.mp3">
    <agent
      id="background"
      image="forest-bg.png" <!-- Substituído por um asset válido -->
      x="0"
      y="0"
      width="100%"
      height="100%"
    />
    <agent
      id="teste"
      text="Ecos Da Floresta Adormecida"
      color="black"
      x="320"
      y="240"
      font-size="70"
    />
    <agent
      id="next_page_text"
      text="Proxima Pagina"
      animation="pulse"
      y="480"
      x="640"
      on_click="goToPage('middle')"
    />
  </page>
</fable>